            <#=d.DT#> z2 = rotation.Z + rotation.Z;
            <#=d.DT#> xy2 = rotation.X * y2;
            <#=d.DT#> xz2 = rotation.X * z2;
            <#=d.DT#> yy2 = rotation.Y * y2;
            <#=d.DT#> zz2 = rotation.Z * z2;
            <#=d.DT#> wy2 = rotation.W * y2;
            <#=d.DT#> wz2 = rotation.W * z2;
